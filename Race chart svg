    <div class="tooltip" id="tooltip"></div>

    <script>
        const tooltip = document.getElementById('tooltip');
        const bars = document.querySelectorAll('.bar');

        bars.forEach(bar => {
            bar.addEventListener('mouseenter', (e) => {
                const race = bar.dataset.race;
                const rate = bar.dataset.rate;
                const count = bar.dataset.count;
                
                tooltip.innerHTML = `
                    <strong>${race}</strong><br>
                    Rate: ${rate}%<br>
                    Cases: ${count}
                `;
                tooltip.classList.add('show');
            });

            bar.addEventListener('mousemove', (e) => {
                tooltip.style.left = e.pageX + 10 + 'px';
                tooltip.style.top = e.pageY + 10 + 'px';
            });

            bar.addEventListener('mouseleave', () => {
                tooltip.classList.remove('show');
            });
        });

        // Animate bars on load
        window.addEventListener('load', () => {
            bars.forEach((bar, index) => {
                const rect = bar.querySelector('rect');
                const originalWidth = rect.getAttribute('width');
                
                rect.setAttribute('width', '0');
                
                setTimeout(() => {
                    rect.style.transition = 'width 0.8s ease-out';
                    rect.setAttribute('width', originalWidth);
                }, index * 150);
            });
        });
    </script>
